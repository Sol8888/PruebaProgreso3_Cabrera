<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="PruebaProgreso3_Cabrera.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:PruebaProgreso3_Cabrera"
    Shell.FlyoutBehavior="Disabled"
    Title="Soledad Cabrera">

    <TabBar>
        
        <ShellContent
            Title="Buscador"
            ContentTemplate="{DataTemplate local:BuscadorAeropuertosPage}"
            Route="BuscadorAeropuertosPage" />

        
        <ShellContent
            Title="Lista"
            ContentTemplate="{DataTemplate local:ListaAeropuertosPage}"
            Route="ListaAeropuertosPage" />
    </TabBar>
    
</Shell>
